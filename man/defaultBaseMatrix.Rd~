\name{defaultBaseMatrix}
\alias{defaultBaseMatrix}
\docType{data}
\title{
defaultBaseMatrix
}
\description{
This dataset is a matrix of decsriptors for each amino acid. It is used
for creating the desrciptors of peptide sequences.
}
\usage{data(defaultBaseMatrix)}
\format{
  A data frame with 24 observations on the following 53 variables.
  \describe{
    \item{\code{naAromAtom}}{Number of aromatic atoms}
    \item{\code{nAromBond}}{Number of aromatic bonds}
    \item{\code{MW}}{molecular weight}
    \item{\code{nRotB}}{number of rotatable bonds}
    \item{\code{apol}}{polarizability}
    \item{\code{bpol}}{a numeric vector}
    \item{\code{PPSA.1}}{a numeric vector}
    \item{\code{PPSA.2}}{a numeric vector}
    \item{\code{PPSA.3}}{a numeric vector}
    \item{\code{PNSA.1}}{a numeric vector}
    \item{\code{PNSA.2}}{a numeric vector}
    \item{\code{PNSA.3}}{a numeric vector}
    \item{\code{DPSA.1}}{a numeric vector}
    \item{\code{DPSA.2}}{a numeric vector}
    \item{\code{DPSA.3}}{a numeric vector}
    \item{\code{FPSA.1}}{a numeric vector}
    \item{\code{FPSA.2}}{a numeric vector}
    \item{\code{FPSA.3}}{a numeric vector}
    \item{\code{FNSA.1}}{a numeric vector}
    \item{\code{FNSA.2}}{a numeric vector}
    \item{\code{FNSA.3}}{a numeric vector}
    \item{\code{WPSA.1}}{a numeric vector}
    \item{\code{WPSA.2}}{a numeric vector}
    \item{\code{WPSA.3}}{a numeric vector}
    \item{\code{WNSA.1}}{a numeric vector}
    \item{\code{WNSA.2}}{a numeric vector}
    \item{\code{WNSA.3}}{a numeric vector}
    \item{\code{RPCG}}{a numeric vector}
    \item{\code{RNCG}}{a numeric vector}
    \item{\code{RPCS}}{a numeric vector}
    \item{\code{RNCS}}{a numeric vector}
    \item{\code{THSA}}{a numeric vector}
    \item{\code{TPSA}}{a numeric vector}
    \item{\code{RHSA}}{a numeric vector}
    \item{\code{RPSA}}{a numeric vector}
    \item{\code{nHBDon}}{number of hydrogen bond donors}
    \item{\code{nHBAcc}}{number of hydrogen bond acceptors}
    \item{\code{TopoPSA}}{a numeric vector}
    \item{\code{XLogP}}{An estimate of the \code{logP} partition coefficient}
    \item{\code{fragC}}{a numeric vector}
    \item{\code{count.AcidicGroups}}{number of acidic groups}
    \item{\code{count.BasicGroups}}{number of basic groups}
    \item{\code{ALogP}}{}
    \item{\code{ALogp2}}{a numeric vector}
    \item{\code{AMR}}{a numeric vector}
    \item{\code{WPATH}}{a numeric vector}
    \item{\code{WPOL}}{a numeric vector}
    \item{\code{PetitjeanNumber}}{a numeric vector}
    \item{\code{nHBAcc.1}}{ numeric vector}
    \item{\code{nHBDon.1}}{a numeric vector}
    \item{\code{helix}}{how helical the amino acid is.}
    \item{\code{pdbhelix}}{how helical a amino acid is using data derived
      from the protein databank}
    \item{\code{CaSD}}{The solvation density near the C-alpha carbon on
      the amino acid}
  }
}
